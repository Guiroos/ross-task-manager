{
    "root": true,
    "env": {
      "browser": false,
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "airbnb-base",
      "plugin:editorconfig/noconflict",
      "plugin:mocha/recommended",
      "airbnb-typescript/base"
    ],
    "parser": "@typescript-eslint/parser",
    "overrides": [
      {
        "files": [
          "*.ts"
        ]
      }
    ],
    "parserOptions": {
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {}
  }